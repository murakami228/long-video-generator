g, #f1c40f, #f39c12);\n    color: #333;\n}\n\n.degree-4 {\n    background: linear-gradient(135deg, #2ecc71, #27ae60);\n}\n\n.degree-5 {\n    background: linear-gradient(135deg, #3498db, #2980b9);\n}\n\n.degree-6 {\n    background: linear-gradient(135deg, #9b59b6, #8e44ad);\n}\n\n/* ã‚³ãƒ¼ãƒ‰å›³ */\n.chord-diagram {\n    width: 100%;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    margin-top: 15px;\n}\n\n.chord-name {\n    font-size: 42px;\n    font-weight: 900;\n    color: #fff;\n    text-shadow: 0 2px 10px rgba(255, 255, 255, 0.3);\n    margin-bottom: 10px;\n}\n\n/* ãƒ•ãƒ¬ãƒƒãƒˆãƒœãƒ¼ãƒ‰ */\n.fretboard {\n    width: 100px;\n    height: 80px;\n    position: relative;\n    background: linear-gradient(180deg, #8B4513 0%, #654321 100%);\n    border-radius: 4px;\n    padding: 8px;\n}\n\n.strings {\n    display: flex;\n    justify-content: space-between;\n    height: 100%;\n    position: absolute;\n    width: calc(100% - 16px);\n    top: 0;\n    left: 8px;\n}\n\n.string {\n    width: 2px;\n    height: 100%;\n    background: linear-gradient(180deg, #f0f0f0, #c0c0c0);\n    border-radius: 1px;\n}\n\n.frets {\n    display: flex;\n    flex-direction: column;\n    justify-content: space-evenly;\n    height: 100%;\n    position: absolute;\n    width: 100%;\n    top: 0;\n    left: 0;\n}\n\n.fret {\n    width: 100%;\n    height: 2px;\n    background: #a0a0a0;\n}\n\n/* æŒ‡ã®ä½ç½® */\n.finger {\n    position: absolute;\n    width: 20px;\n    height: 20px;\n    background: linear-gradient(135deg, #3498db, #2980b9);\n    border-radius: 50%;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    font-size: 12px;\n    font-weight: 700;\n    color: white;\n    transform: translate(-50%, -50%);\n    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.4);\n}\n\n/* ãƒãƒ¬ãƒ¼ã‚³ãƒ¼ãƒ‰è¡¨ç¤º */\n.barre {\n    position: absolute;\n    left: 5%;\n    width: 90%;\n    height: 18px;\n    background: linear-gradient(135deg, #3498db, #2980b9);\n    border-radius: 9px;\n    transform: translateY(-50%);\n    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.4);\n}\n\n/* ã‚³ãƒ¼ãƒ‰ã‚¿ã‚¤ãƒ— */\n.chord-type {\n    font-size: 14px;\n    color: #a0a0a0;\n    margin-top: 10px;\n    padding: 4px 12px;\n    background: rgba(255, 255, 255, 0.1);\n    border-radius: 12px;\n}\n\n/* ãƒ†ã‚£ãƒƒãƒ—ãƒœãƒƒã‚¯ã‚¹ */\n.tip-box {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    gap: 12px;\n    margin-top: 20px;\n    padding: 16px 24px;\n    background: linear-gradient(135deg, rgba(46, 204, 113, 0.2), rgba(39, 174, 96, 0.2));\n    border: 2px solid rgba(46, 204, 113, 0.4);\n    border-radius: 12px;\n}\n\n.tip-icon {\n    font-size: 28px;\n}\n\n.tip-text {\n    font-size: 22px;\n    font-weight: 700;\n    color: #2ecc71;\n}\n\n/* ã‚¢ãƒ‹ãƒ¡ãƒ¼ã‚·ãƒ§ãƒ³ç”¨ï¼šåˆæœŸçŠ¶æ…‹ã§éè¡¨ç¤º */\n#item-1,\n#item-2,\n#item-3,\n#item-4,\n#item-5,\n#item-6,\n#item-7 {\n    opacity: 0;\n}\n\n/* ãƒ›ãƒãƒ¼ã‚¨ãƒ•ã‚§ã‚¯ãƒˆï¼ˆãƒ—ãƒ¬ãƒ“ãƒ¥ãƒ¼ç”¨ï¼‰ */\n.chord-card:hover {\n    transform: scale(1.02);\n    border-color: rgba(255, 255, 255, 0.3);\n    background: rgba(255, 255, 255, 0.12);\n}",
